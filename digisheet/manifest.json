{
    "name": "Digisheet Auto Regist",
    "version": "0.0.2",
    "description": "Make a string to a code block.",
    "manifest_version": 2,
    "permissions": [
        "contextMenus",
        "activeTab"
      ],
    "background": {
      "scripts": [
        "background.js",
        "jquery-3.6.0.min.js"
      ],
      "persistent": true
    },
    "content_scripts": [
      {
        "matches": [
          "http://*/*",
          "https://*/*"
        ],
        "js": [
          "jquery-3.6.0.min.js",
          "content.js"
        ],
        "all_frames": true
      }
    ],
    "commands": {
      "code-block": {
        "suggested_key": {
          "default": "Ctrl+Shift+Y",
          "mac": "Command+Shift+Y"
        },
        "description": "Make selected text to a code block."
      }
    }
  }